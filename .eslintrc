{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "mocha": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "class-methods-use-this": 0,
    "no-console": 0
  }
}
